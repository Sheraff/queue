{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "server/*": [
        "./*"
      ]
    }
  },
  "include": [
    "vite.config.ts",
    "server",
    "tailwind.config.ts",
    "postcss.config.js",
  ]
}